{
  "manifest_version": 2,
  "name": "Milk Corp.",
  "version": "1.2",
  "author":"Jayson Tian",
  "Content-Security-Policy":" script-src https://cdn.example.com/scripts/; object-src 'none'",
  "browser_action":{
    "default_icon":"images/icon.png",

  	"default_title":"Milk Corp."
  },

  "background":{
  	"scripts": ["background.js"]
  },

  "permissions":[
  	"activeTab"
  ],
  "content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "css": ["css/bg.css"],
	    "js": ["content.js"]
	  }
	]
}

